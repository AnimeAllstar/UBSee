{
  "name": "ubsee",
  "version": "1.0.0",
  "description": "Creates interactive graphs for UBC subjects and courses",
  "main": "index.js",
  "engines": {
    "node": "14.16.1"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "add": "node scripts/add.js",
    "test": "node scripts/test.js",
    "fix": "node scripts/fix.js",
    "concat": "concat -o ./public/js/ubsee.js ./public/js/graph-initializer.js ./public/js/node-event-handler.js ./public/js/page-event-handler.js ./public/js/select2-event-handler.js ./public/js/util.js",
    "uglify": "uglifyjs public/js/ubsee.js -o public/js/ubsee.min.js",
    "build": "npm run concat && npm run uglify",
    "json-mongo": "node scripts/json-mongo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnimeAllstar/UBSee.git"
  },
  "keywords": [
    "ubsee",
    "UBC",
    "Nodejs",
    "express",
    "GoJS"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AnimeAllstar/UBSee/issues"
  },
  "homepage": "https://github.com/AnimeAllstar/UBSee#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mongodb": "^3.6.10",
    "nunjucks": "^3.2.3",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "concat": "^1.0.3",
    "jsonfile": "^6.1.0",
    "needle": "^2.6.0",
    "nodemon": "^2.0.7",
    "sort-json": "^2.0.0",
    "uglify-js": "^3.13.10"
  }
}